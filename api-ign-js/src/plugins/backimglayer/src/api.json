{
  "url": {
    "name": "backimglayer",
    "separator": "*"
  },
  "constructor": "M.plugin.BackImgLayer",
  "parameters": [{
    "type": "object",
    "properties": [{
      "type": "simple",
      "name": "position",
      "position": 0,
      "possibleValues": ["TL", "TR", "BR", "BL"]
    }, {
      "type": "number",
      "name": "layerId",
      "position": 1
    }, {
      "type": "boolean",
      "name": "layerVisibility",
      "position": 2
    }, {
      "type": "number",
      "name": "columnsNumber",
      "position": 3
    }, {
      "type": "object",
      "name": "layerOpts",
      "properties": [{
        "type": "simple",
        "name": "id",
        "value": "mapa"
      }, {
        "type": "simple",
        "name": "preview",
        "value": "../src/facade/assets/images/svqmapa.png"
      }, {
        "type": "simple",
        "name": "title",
        "value": "Mapa"
      }, {
        "type": "object",
        "name": "layers",
        "properties": [{
          "type": "simple",
          "name": "url",
          "value": "http://www.ign.es/wmts/ign-base?"
        }, {
          "type": "simple",
          "name": "name",
          "value": "IGNBaseTodo"
        }, {
          "type": "simple",
          "name": "legend",
          "value": "Mapa IGN"
        }, {
          "type": "simple",
          "name": "matrixSet",
          "value": "GoogleMapsCompatible"
        }, {
          "type": "boolean",
          "name": "transparent",
          "value": "false"
        }, {
          "type": "boolean",
          "name": "displayInLayerSwitcher",
          "value": "false"
        }, {
          "type": "boolean",
          "name": "queryable",
          "value": "false"
        }, {
          "type": "boolean",
          "name": "visible",
          "value": "true"
        }, {
          "type": "simple",
          "name": "format",
          "value": "image/jpeg"
        }]
      }]
    }]
  }],
  "files": {
    "ol": {
      "scripts": [
        "backimglayer.ol.min.js"
      ],
      "styles": [
        "backimglayer.ol.min.css"
      ]
    }
  },
  "metadata": {
    "name": "backimglayer",
    "description": "Sets background layer",
    "text": "Shows different options for background layer as clickable images and changes background layer on click on an option.",
    "version": "1.0.0",
    "date": "3rd September 2019",
    "author": "CNIG",
    "org": "CNIG",
    "tags": "mapea,plugin",
    "icon": "./facade/assets/icons/icons.svg"
  }
}
